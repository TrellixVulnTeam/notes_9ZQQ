import React from 'react';
import Home from './home';

class App extends React.Component{

	constructor(){
		super();
		this.state = {
			name : 'Tohid'
		};
		console.log("constructor");
	}

 componentDidMount(){
 	console.log("componentDidMount")
 }
	render(){
		  console.log("Render");
		return(
			 <>
            
			  <Home /> 
			  { console.log(this.state.name)}   

			 </>

			)
	}
}

export default App;

















