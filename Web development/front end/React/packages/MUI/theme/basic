import React from 'react';
import ReactDOM from 'react-dom';
import './index.css';
import { createTheme, ThemeProvider } from '@material-ui/core';
import App from './App';

const theme = createTheme({
  palette : {
    primary : {
      main : "#FF0000"
    }
  }
});

ReactDOM.render(

  <ThemeProvider theme={theme}>
    <App />
  </ThemeProvider>, 
  document.getElementById('root')
);

