<script type="text/javascript">


const add = (function () {
  let counter = 0;
  return function () {counter += 1; return counter}
})();


console.log(add())

function myFUnction(){
  document.getElementById('demo').innerHTML = add();
}



</script>

<p id="demo"></p>

