import React from 'react';



const numbers = [1,2,3,4];
// list component
const listItem = numbers.map((number)=> 
              		<li> {number} </li>
              )

class App extends React.Component{
 

	render(){
		return(
			<>
			  // displying list
              	<ul> {listItem} </ul>	
			</>
			)
	}
}

export default App;

