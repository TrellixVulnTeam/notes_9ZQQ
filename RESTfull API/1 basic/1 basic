const express = require('express');
const app  = express();



// use() support get, post,delete, patch  all the verbs  
app.use((req, res, next)=> {
	res.status(200).json({
		message : "it works"
	})
})




app.listen(3000, ()=> {
	console.log("server is start");
})



// search on postman
// localhost:3000 



