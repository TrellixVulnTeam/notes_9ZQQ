

// simplified syntax
const element = (
	<h1 className = "greeting">
		Hello, world 
	</h1>
	);



 // same working as before
const element = React.createElement(
	'h1'
	{className : 'greeting'},
	'Hello world'
	);



 // same working as before
const element = {
	type : 'h1',
	props : {
		classname : 'greeting',
		children : 'Hello world'
	}
};



