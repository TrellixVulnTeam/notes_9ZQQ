<div class="jumbotron">
  <a class="btn btn-info" href="/insert">Add New</a>


    <div class="continer">
    	<table class="table table-striped table-bordered">

    		<thead>
    			<tr class="bg-secondary">
    				<th> Name </th>
    				<th> Email </th>
    				<th> </th>
    			</tr>
    		</thead>

    		<tbody>

    			{{#each students }}
    				<tr>
    					<td> {{this.name}} </td>
    					<td> {{this.email}} </td>
    					<td> 
    						<a class="btn btn-info" href="/update/{{this.id}}">Update</a> 
    						<a class="btn btn-danger" href="/delete/{{this.id}}">Del</a> 

    					</td>

    				</tr>

    			{{/each}}


    		</tbody>
    	</table>
    </div>




</div>